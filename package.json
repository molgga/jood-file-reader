{
  "name": "jood-file-reader",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "ng:dev": "ng serve dev",
    "packages:build": "ng build packages --prod",
    "packages:test": "ng test packages --code-coverage",
    "packages:doc": "jsdoc -c jsdoc.json -r -d docs",
    "page:example-dist": "ng build dev",
    "page:example-make": "copyfiles -u 2 ./dist/dev/* ./dist/dev/**/* ./docs",
    "page:document-dist": "jsdoc -c jsdoc.json -r -d docs/documents",
    "page:clear": "rm -rf docs",
    "page:expose": "npm run page:example-dist && npm run page:example-make && npm run page:document-dist",
    "ci:test": "ng test packages --watch=false --code-coverage --browsers=ChromeHeadless && codecov",
    "ci:build": "npm run packages:build",
    "ci:deploy-before": "npm run page:clear && npm run page:expose && npm run ci:build",
    "ci:deploy-pre-test": "npm run ci:test && npm run ci:deploy-before"
  },
  "dependencies": {
    "@angular/animations": "~10.0.6",
    "@angular/common": "~10.0.6",
    "@angular/compiler": "~10.0.6",
    "@angular/core": "~10.0.6",
    "@angular/forms": "~10.0.6",
    "@angular/material": "^10.1.1",
    "@angular/platform-browser": "~10.0.6",
    "@angular/platform-browser-dynamic": "~10.0.6",
    "@angular/router": "~10.0.6",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.4",
    "@angular-devkit/build-ng-packagr": "^0.1000.4",
    "@angular/cdk": "^10.1.1",
    "@angular/cli": "^10.0.4",
    "@angular/compiler-cli": "~10.0.6",
    "@angular/language-service": "~10.0.6",
    "@jood/jsdoc-theme": "^0.1.13",
    "@types/jasmine": "~3.5.11",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.0.27",
    "better-docs": "^2.3.0",
    "codecov": "^3.6.4",
    "codelyzer": "^6.0.0",
    "copyfiles": "^2.3.0",
    "custom-event-polyfill": "^1.0.7",
    "exifreader": "^3.12.2",
    "intersection-observer": "^0.11.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "jsdoc": "3.6.4",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.2",
    "ng-packagr": "^10.0.3",
    "protractor": "~7.0.0",
    "rxjs": "^6.5.4",
    "ts-node": "~8.10.2",
    "tsickle": "^0.39.1",
    "tslint": "~6.1.2",
    "typescript": "~3.9.7"
  },
  "keywords": [
    "javascript",
    "typescript",
    "es6",
    "frontend",
    "file",
    "fileReader",
    "fileChunk",
    "blob",
    "image",
    "resize"
  ]
}
